{
    "name": "xkcd NTmin",
    "version": "1.0.0",
    "description": "Minimalist daily xkcd new tab page.",
    "manifest_version": 3,
    "icons": {
        "128": "icon128.png"
    },
    "chrome_url_overrides" : {
        "newtab": "newtab/newtab.html"
    },
    "permissions": [
        "geolocation"
    ],
    "host_permissions": [
        "https://xkcd.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}